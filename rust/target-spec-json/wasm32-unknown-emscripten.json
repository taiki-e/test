{
  "arch": "wasm32",
  "crt-objects-fallback": "wasm",
  "data-layout": "e-m:e-p:32:32-i64:64-n32:64-S128",
  "default-hidden-visibility": true,
  "dll-prefix": "",
  "dll-suffix": ".wasm",
  "dynamic-linking": true,
  "eh-frame-header": false,
  "emit-debug-gdb-scripts": false,
  "exe-suffix": ".js",
  "executables": true,
  "has-elf-tls": true,
  "is-builtin": true,
  "is-like-emscripten": true,
  "is-like-wasm": true,
  "limit-rdylib-exports": false,
  "linker-flavor": "em",
  "lld-flavor": "wasm",
  "llvm-target": "wasm32-unknown-emscripten",
  "max-atomic-width": 64,
  "only-cdylib": true,
  "os": "emscripten",
  "post-link-args": {
    "em": [
      "-s",
      "ERROR_ON_UNDEFINED_SYMBOLS=1",
      "-s",
      "ASSERTIONS=1",
      "-s",
      "ABORTING_MALLOC=0",
      "-Wl,--fatal-warnings"
    ]
  },
  "pre-link-args": {
    "gcc": [
      "-Wl,-z",
      "-Wl,stack-size=1048576",
      "-Wl,--stack-first",
      "-Wl,--allow-undefined",
      "-Wl,--fatal-warnings",
      "-Wl,--no-demangle",
      "--export-dynamic"
    ],
    "wasm-ld": [
      "-z",
      "stack-size=1048576",
      "--stack-first",
      "--allow-undefined",
      "--fatal-warnings",
      "--no-demangle"
    ]
  },
  "relocation-model": "static",
  "simd-types-indirect": false,
  "singlethread": true,
  "target-family": [
    "unix"
  ],
  "target-pointer-width": "32",
  "tls-model": "local-exec"
}
