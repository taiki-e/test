{
  "abi": "fortanix",
  "arch": "x86_64",
  "cpu": "x86-64",
  "data-layout": "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-f80:128-n8:16:32:64-S128",
  "env": "sgx",
  "executables": true,
  "features": "+rdrnd,+rdseed,+lvi-cfi,+lvi-load-hardening",
  "is-builtin": true,
  "linker": "rust-lld",
  "linker-flavor": "ld.lld",
  "llvm-args": [
    "--x86-experimental-lvi-inline-asm-hardening"
  ],
  "llvm-target": "x86_64-elf",
  "max-atomic-width": 64,
  "os": "unknown",
  "override-export-symbols": [
    "sgx_entry",
    "HEAP_BASE",
    "HEAP_SIZE",
    "RELA",
    "RELACOUNT",
    "ENCLAVE_SIZE",
    "CFGDATA_BASE",
    "DEBUG",
    "EH_FRM_HDR_OFFSET",
    "EH_FRM_HDR_LEN",
    "EH_FRM_OFFSET",
    "EH_FRM_LEN",
    "TEXT_BASE",
    "TEXT_SIZE"
  ],
  "position-independent-executables": true,
  "pre-link-args": {
    "ld.lld": [
      "-e",
      "elf_entry",
      "-Bstatic",
      "--gc-sections",
      "-z",
      "text",
      "-z",
      "norelro",
      "--no-undefined",
      "--error-unresolved-symbols",
      "--no-undefined-version",
      "-Bsymbolic",
      "--export-dynamic",
      "-u",
      "__rust_abort",
      "-u",
      "__rust_c_alloc",
      "-u",
      "__rust_c_dealloc",
      "-u",
      "__rust_print_err",
      "-u",
      "__rust_rwlock_rdlock",
      "-u",
      "__rust_rwlock_unlock",
      "-u",
      "__rust_rwlock_wrlock"
    ]
  },
  "relax-elf-relocations": true,
  "target-pointer-width": "64",
  "vendor": "fortanix"
}
