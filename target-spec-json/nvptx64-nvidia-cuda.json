{
  "arch": "nvptx64",
  "cpu": "sm_30",
  "data-layout": "e-i64:64-i128:128-v16:16-v32:32-n16:32:64",
  "dll-prefix": "",
  "dll-suffix": ".ptx",
  "dynamic-linking": true,
  "exe-suffix": ".ptx",
  "executables": true,
  "is-builtin": true,
  "linker": "rust-ptx-linker",
  "linker-flavor": "ptx-linker",
  "linker-is-gnu": false,
  "llvm-target": "nvptx64-nvidia-cuda",
  "max-atomic-width": 64,
  "merge-functions": "disabled",
  "obj-is-bitcode": true,
  "only-cdylib": true,
  "os": "cuda",
  "panic-strategy": "abort",
  "target-pointer-width": "64",
  "vendor": "nvidia"
}
