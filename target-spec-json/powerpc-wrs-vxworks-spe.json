{
  "arch": "powerpc",
  "crt-static-allows-dylibs": true,
  "crt-static-default": true,
  "crt-static-respected": true,
  "data-layout": "E-m:e-p:32:32-i64:64-n32",
  "dynamic-linking": true,
  "env": "gnu",
  "exe-suffix": ".vxe",
  "executables": true,
  "features": "+secure-plt,+msync",
  "has-elf-tls": true,
  "has-rpath": true,
  "is-builtin": true,
  "linker": "wr-c++",
  "llvm-target": "powerpc-unknown-linux-gnuspe",
  "max-atomic-width": 32,
  "os": "vxworks",
  "pre-link-args": {
    "gcc": [
      "-mspe",
      "--secure-plt"
    ]
  },
  "target-endian": "big",
  "target-family": [
    "unix"
  ],
  "target-mcount": "_mcount",
  "target-pointer-width": "32",
  "vendor": "wrs"
}
