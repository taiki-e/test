FROM ubuntu:20.04

RUN apt-get -o Dpkg::Use-Pty=0 update -qq && DEBIAN_FRONTEND=noninteractive apt-get -o Dpkg::Use-Pty=0 install -y --no-install-recommends \
        binfmt-support \
        ca-certificates \
        curl \
        file \
        g++ \
        g++-mipsel-linux-gnu \
        libc6-dev \
        libc6-dev-mipsel-cross \
        make \
    && rm -rf /var/lib/apt/lists/*

COPY qemu.sh /
RUN /qemu.sh mipsel

ENV CARGO_TARGET_MIPSEL_UNKNOWN_LINUX_GNU_LINKER=mipsel-linux-gnu-gcc \
    CARGO_TARGET_MIPSEL_UNKNOWN_LINUX_GNU_RUNNER=qemu-mipsel \
    CC_mipsel_unknown_linux_gnu=mipsel-linux-gnu-gcc \
    CXX_mipsel_unknown_linux_gnu=mipsel-linux-gnu-g++ \
    OBJDUMP=mipsel-linux-gnu-objdump \
    QEMU_LD_PREFIX=/usr/mipsel-linux-gnu
