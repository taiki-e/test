FROM ubuntu:20.04

RUN apt-get -o Dpkg::Use-Pty=0 update -qq && DEBIAN_FRONTEND=noninteractive apt-get -o Dpkg::Use-Pty=0 install -y --no-install-recommends \
        binfmt-support \
        ca-certificates \
        curl \
        file \
        g++ \
        g++-arm-linux-gnueabihf \
        libc6-dev \
        libc6-dev-armhf-cross \
        make \
    && rm -rf /var/lib/apt/lists/*

COPY qemu.sh /
RUN /qemu.sh arm

